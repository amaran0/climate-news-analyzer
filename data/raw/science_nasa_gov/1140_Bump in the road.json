  \n                + now.getHours() + \":\"  \n                + gmu.addZero(now.getMinutes()) + \":\" \n                + gmu.addZero(now.getSeconds()) + \" .\" \n                + now.getMilliseconds();\n      var event_label = window.location.href + \" (\" + date_string + \")\"\n      //if(!cookie_exists && param && param.length > 0){\n      console.log('param: ' + param);\n      console.log('cookie_exists: ' + cookie_exists);\n      if(param && param.length > 0 && cookie_exists != param){\n        console.log('create GMU cookie')\n        var date = new Date();\n        date.setTime(date.getTime()+(10*60*1000));  //10 minutes\n        var expires = \"; expires=\"+date.toGMTString();\n        document.cookie = \"GMU_Visitor=\"+param+expires+'; path=/';\n        _sendEvent('GMU', param, event_label, now.getTime());\n      } else if (cookie_exists){\n        console.log('use existing GMU cookie')\n        _sendEvent('GMU', cookie_exists, event_label, now.getTime());\n      } else {\n        //do nothing\n      }\n    },\n  \n    analyticsReady: function(){\n      if(typeof(\"_sendEvent\") == 'undefined'){\n        console.log(\"analytics not loaded. wait 300ms.\")\n        window.setTimeout(gmu.analyticsReady, 300);\n      } else {\n        console.log(\"analytics ok.\")\n        gmu.checkGMU()\n      }\n    }\n  }\n  \n  $(document).ready(function(){\n    gmu.analyticsReady();\n  })\n</script>\n\n<!-- Google Tag Manager --><script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src='https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);})(window,document,'script','dataLayer','GTM-PZG28CN');</script><!-- End Google Tag Manager -->\n\n\n<script>\n  if (typeof window.CookieConsent != 'undefined'){\n    window.dataLayer = window.dataLayer || [];\n    function gtag(){dataLayer.push(arguments);}\